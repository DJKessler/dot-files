# i3 config file (v4) for MacbookPro running ubuntu 14.04

# make mouse accel be 1-to-1
exec --no-startup-id "xset mouse 1 1"

bindsym XF86MonBrightnessDown exec "xbacklight -dec 10"
bindsym XF86MonBrightnessUp exec "xbacklight -inc 10"

bindsym $mod+Shift+F3 exec --no-startup-id "~/.unixrc/i3-scripts/i3-undock"
bindsym $mod+Shift+F4 exec --no-startup-id "~/.unixrc/i3-scripts/i3-dock"
