[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	default = simple
[core]
	excludesfile = ~/.dot-files/git.ignore
	autocrlf = input
	editor = vim -c \"setlocal spell spelllang=en_us\"
	pager = cat
[clangFormat]
  style = file
[color]
	ui = true
[mergetool]
	keepBackup = false
